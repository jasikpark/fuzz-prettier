{
  "name": "fuzz-prettier",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "fuzz-babel-simple": "jazzer ./dist/fuzz-babel-simple.js corpus -- -max_total_time=60",
    "fuzz-babel-idempotent": "jazzer ./dist/fuzz-babel-idempotent.js corpus -- -max_total_time=60",
    "fuzz-html-simple": "jazzer ./dist/fuzz-html-simple.js corpus -- -max_total_time=60",
    "fuzz-html-idempotent": "jazzer ./dist/fuzz-html-idempotent.js corpus -- -max_total_time=60",
    "fuzz-go-template-simple": "jazzer ./dist/fuzz-go-template-simple.js corpus -- -max_total_time=60",
    "fuzz-toml-simple": "jazzer ./dist/fuzz-toml-simple.js corpus -- -max_total_time=60",
    "fuzz-go-template-idempotent": "jazzer ./dist/fuzz-go-template-idempotent.js corpus -- -max_total_time=60",
    "fuzz-astro-simple": "jazzer ./dist/fuzz-astro-simple.js corpus -- -max_total_time=60",
    "fuzz-astro-idempotent": "jazzer ./dist/fuzz-astro-idempotent.js corpus -- -max_total_time=60",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "fuzz": "NODE_OPTIONS=--experimental-vm-modules JAZZER_FUZZ=1 jest",
    "coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "jest": {
    "projects": [
      {
        "displayName": "test"
      },
      {
        "displayName": {
          "name": "Jazzer.js",
          "color": "cyan"
        },
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/*.fuzz.[jt]s"
        ],
        "testRunner": "@jazzer.js/jest-runner",
        "extensionsToTreatAsEsm": [
          ".ts"
        ],
        "moduleNameMapper": {
          "^(\\.{1,2}/.*)\\.js$": "$1"
        },
        "transform": {
          "^.+\\.ts?$": [
            "ts-jest",
            {
              "useESM": true
            }
          ]
        }
      }
    ]
  },
  "author": "Caleb Jasik",
  "license": "ISC",
  "devDependencies": {
    "@jazzer.js/core": "1.7.0",
    "@jazzer.js/jest-runner": "^2.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "18.18.4",
    "@types/prettier": "2.7.3",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "prettier-plugin-go-template": "0.0.15",
    "ts-jest": "^29.1.1",
    "typescript": "5.0.2"
  },
  "dependencies": {
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-toml": "^1.0.0"
  },
  "packageManager": "pnpm@8.8.0+sha256.d713a5750e41c3660d1e090608c7f607ad00d1dd5ba9b6552b5f390bf37924e9"
}
